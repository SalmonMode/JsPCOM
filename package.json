{
  "name": "tspcom",
  "version": "1.0.0",
  "description": "TypeScript and JavaScript page component object framework for Selenium",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalmonMode/TsPCOM.git"
  },
  "keywords": [
    "javascript",
    "selenium",
    "automation",
    "test",
    "testing",
    "check",
    "checking",
    "verify",
    "verification",
    "typescript",
    "page",
    "component",
    "framework",
    "pom",
    "pcom"
  ],
  "author": "Chris NeJame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SalmonMode/TsPCOM/issues"
  },
  "homepage": "https://github.com/SalmonMode/TsPCOM#readme",
  "devDependencies": {
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/selenium-webdriver": "^4.0.9",
    "reflect-metadata": "^0.1.13",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "tslib": "^2.0.3"
  }
}
